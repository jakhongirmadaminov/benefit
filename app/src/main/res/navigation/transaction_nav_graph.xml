<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/transaction_nav_graph.xml"
    app:startDestination="@id/transactionFragment">


    <fragment
        android:id="@+id/transactionFragment"
        android:name="com.example.benefit.ui.transactions_history.transaction_bsd.TransactionFragment"
        android:label=""
        tools:layout="@layout/fragment_transaction">

        <argument android:name="argTransactionDTO" app:argType="com.example.benefit.remote.models.TransactionDTO"  />

        <action
            android:id="@+id/action_transactionFragment_to_cardMakeDepositFromAnyCardFragment"
            app:destination="@id/cardMakeDepositFromAnyCardFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
        <action
            android:id="@+id/action_transactionFragment_to_cardMakeDepositFromMyCardFragment"
            app:destination="@id/cardMakeDepositFromMyCardFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />

        <action
            android:id="@+id/action_transactionFragment_to_cardDepositAskFriendsFragment"
            app:destination="@id/cardDepositAskFriendsFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>

</navigation>